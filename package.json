{
  "name": "roblox-integration-hub",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "description": "This app connects Roblox Studio with other apps and tools",
  "main": "./out/main/index.js",
  "scripts": {
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev-watch": "electron-vite dev --watch",
    "debug": "electron-vite dev --sourcemap --remote-debugging-port=9222",
    "build": "electron-vite build",
    "build-cheking": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "test": "jest",
    "test:e2e": "jest .e2e-spec.ts$",
    "test:unit": "jest \\.spec.ts$"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.1.0",
    "@doubleshot/nest-electron": "^0.2.5",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@nestjs/common": "^10.4.6",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.6",
    "@nestjs/event-emitter": "^2.1.1",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/microservices": "^10.4.6",
    "@nestjs/platform-express": "^10.4.6",
    "@parcel/watcher": "^2.4.1",
    "axios": "^1.7.7",
    "chokidar": "^4.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "electron-updater": "^6.1.7",
    "framer-motion": "^11.5.5",
    "fs-extra": "^11.2.0",
    "is-online": "^11.0.0",
    "jimp": "^1.6.0",
    "jwt-decode": "^4.0.0",
    "keytar": "^7.9.0",
    "micromatch": "^4.0.8",
    "mime-types": "^2.1.35",
    "next-themes": "^0.4.3",
    "nookies": "^2.5.2",
    "p-event": "^6.0.1",
    "p-limit": "^6.1.0",
    "p-queue": "^8.0.1",
    "p-retry": "^4.6.2",
    "p-timeout": "^6.1.3",
    "react": "^18.3.1",
    "react-custom-events": "^1.1.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.2",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@eslint-react/eslint-plugin": "^1.15.2",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/core": "^1.7.40",
    "@swc/jest": "^0.2.37",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^15.0.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.2",
    "@vitejs/plugin-react": "^4.3.1",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^9.14.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "jest": "^29.5.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vite-tsconfig-paths": "^5.1.0"
  }
}
